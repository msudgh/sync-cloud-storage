{
  "name": "sync-cloud-storage",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Masoud Ghorbani <contact@mgh.space>",
    "url": "https://mgh.space"
  },
  "keywords": [
    "sync",
    "cloud",
    "storage",
    "bucket",
    "serverless",
    "aws",
    "s3"
  ],
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json",
    "build:watch": "tsc -p tsconfig.build.json -w",
    "test": "jest --verbose --no-cache",
    "test:watch": "jest --no-cache --watch",
    "test:coverage": "jest --no-cache --coverage",
    "format": "prettier --write \"**/*.{ts,js,md,json,yaml,yml}\"",
    "lint": "eslint . --debug --ext .ts --ext .js",
    "lint:fix": "eslint . --fix --ext .ts --ext .js",
    "docs:build": "pnpm run build && rm -rf ./docs && ./node_modules/.bin/jsdoc -c jsdoc.json",
    "docs:start": "pnpm run docs:build && ./node_modules/.bin/http-server -c-1 -o docs"
  },
  "devDependencies": {
    "@aws-sdk/credential-providers": "^3.499.0",
    "@faker-js/faker": "^8.3.1",
    "@smithy/shared-ini-file-loader": "^2.3.1",
    "@smithy/types": "2.9.1",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.5",
    "@types/serverless": "3.12.21",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "aws-sdk": "2.1541.0",
    "colors": "^1.4.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "fast-check": "^3.15.0",
    "fs-extra": "11.2.0",
    "http-server": "14.1.1",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "jsdoc": "4.0.2",
    "prettier": "3.2.4",
    "serverless": "3.38.0",
    "ts-jest": "29.1.1",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.496.0",
    "@aws-sdk/lib-storage": "3.496.0",
    "mrmime": "2.0.0",
    "zod": "3.22.4"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run lint:fix",
      "npm run format"
    ],
    "*.{json,md,yaml}": [
      "npm run format"
    ]
  }
}
