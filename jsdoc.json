{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["./README.md", "./dist"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs",
    "recurse": true
  }
}
