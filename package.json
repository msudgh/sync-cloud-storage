{
  "name": "cloud-bucket-sync",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": {
    "name": "Masoud Ghorbani <contact@mgh.space>",
    "url": "https://mgh.space"
  },
  "keywords": [
    "serverless",
    "aws",
    "s3",
    "bucket",
    "sync",
    "cloud"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "npm run build:docs",
    "build:docs": "pnpm dlx jsdoc -c jsdoc.json",
    "start:docs": "pnpm run build:docs && pnpm dlx http-server -c-1 -o docs",
    "build:watch": "tsc -p tsconfig.build.json -w",
    "test": "jest --coverage",
    "format": "prettier --write \"**/*.{ts,md,json,yaml}\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix --ext .ts",
    "prepare": "is-ci || husky install"
  },
  "devDependencies": {
    "@smithy/types": "2.9.1",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.5",
    "@types/serverless": "3.12.21",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "aws-sdk": "2.1540.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "fs-extra": "11.2.0",
    "http-server": "14.1.1",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "jsdoc": "4.0.2",
    "prettier": "3.2.4",
    "serverless": "3.38.0",
    "ts-jest": "29.1.1",
    "ts-to-jsdoc": "1.5.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.496.0",
    "@aws-sdk/lib-storage": "3.496.0",
    "mrmime": "2.0.0",
    "zod": "3.22.4"
  },
  "lint-staged": {
    "*.{ts}": [
      "npm run lint:fix",
      "npm run format"
    ],
    "*.{json,md,yaml}": [
      "npm run format"
    ]
  }
}
